{
  "name": "jupyter-js-widgets",
  "version": "0.0.13",
  "description": "Jupyter widget Javascript",
  "author": "Project Jupyter",
  "license": "BSD-3-Clause",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ipython/ipywidgets.git"
  },
  "scripts": {
    "build": "npm run css",
    "precss": "rimraf ./css/",
    "css": "node build_css.js",
    "test": "npm run test:unit && npm run test:examples",
    "test:unit": "npm run test:unit:firefox && npm run test:unit:chrome",
    "test:unit:firefox": "karma start karma.conf.js --browsers Firefox --log-level debug",
    "test:unit:chrome": "karma start karma.conf.js --log-level debug",
    "test:examples": "npm run test:examples:web1 && npm run test:examples:web2 && npm run test:examples:web3",
    "test:examples:web1": "cd examples/web && npm install && npm test",
    "test:examples:web2": "cd examples/web2 && npm install && npm test",
    "test:examples:web3": "cd examples/web3 && npm install && npm test",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "bower": "^1.5.3",
    "chai": "^3.4.1",
    "clean-css": "^3.4.6",
    "font-awesome": "^4.5.0",
    "karma": "^0.13.15",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.3",
    "karma-sinon-chai": "^1.1.0",
    "karma-webpack": "^1.7.0",
    "less": "^2.5.3",
    "mocha": "^2.3.4",
    "rimraf": "^2.4.1",
    "sinon": "^1.17.2",
    "spawn-sync": "^1.0.14",
    "webpack": "^1.12.11"
  },
  "dependencies": {
    "backbone": "1.2.0",
    "bootstrap": "^3.3.5",
    "jquery": "^2.1.4",
    "jquery-ui": "^1.10.5",
    "underscore": "^1.8.3"
  }
}
